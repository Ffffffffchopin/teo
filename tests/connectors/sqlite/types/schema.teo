connector {
  provider .sqlite
  url "sqlite:test-connectors-sqlite-types.sqlite"
}

server {
  bind ("0.0.0.0", 4014)
}

enum Sex {
  MALE
  FEMALE
}

model Support {
  @id @autoIncrement @readonly
  id: Int
  int32: Int32?
  int64: Int64?
  float32: Float32?
  float64: Float64?
  bool: Bool?
  string: String?
  date: Date?
  dateTime: DateTime?
  decimal: Decimal?
  sex: Sex?
}
